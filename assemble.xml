<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>bundle</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>target/dependency</directory>
			<outputDirectory></outputDirectory>	 		 
		</fileSet>
		<fileSet>
			<directory>models</directory>
			<outputDirectory>models</outputDirectory>	 		 
		</fileSet>
		<fileSet>
			<directory>cfg</directory>
			<outputDirectory>cfg</outputDirectory>	 		 
		</fileSet>
		<fileSet>
			<directory>en</directory>
			<outputDirectory>en</outputDirectory>	 		 
		</fileSet>
		<fileSet>
			<directory>ont</directory>
			<outputDirectory>ont</outputDirectory>	 		 
		</fileSet>
	</fileSets>
	<files>
	    <file>
	      <source>Procfile</source>
	      <outputDirectory></outputDirectory>
	    </file>
	    <file>
	      <source>LICENSE</source>
	      <outputDirectory></outputDirectory>
	    </file>	   
	    <file>
	      <source>STATUS</source>
	      <outputDirectory></outputDirectory>
	    </file>	  
	    <file>
	      <source>BUGS</source>
	      <outputDirectory></outputDirectory>
	    </file>	  	    
	    <file>
	      <source>target/sedro-1.0.9999.war</source>
	      <outputDirectory></outputDirectory>
	    </file>
  </files>
</assembly>